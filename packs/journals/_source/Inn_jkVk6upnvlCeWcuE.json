    },
    {
      "name": "[S14] Private Dining Rooms",
      "type": "text",
      "text": {
        "content": "<p>The dining room with the fireplace is used all year round.</p>",
        "format": 1
      },
      "_id": "g1TTy6ad2vWGkQKT",
